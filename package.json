{
  "name": "vite-react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "commitlint": "commitlint --edit",
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore --check \"**/*.+(js|json|ts|tsx)\"",
    "format:fix": "prettier --ignore-path .prettierignore --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "lint": "eslint --max-warnings=0",
    "lint:fix": "eslint --max-warnings=0 --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "validate": "npm-run-all --parallel typecheck lint format",
    "validate-and-build": "npm-run-all validate build",
    "deploy": "npm-run-all validate-and-build serve"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@tanstack/react-router": "1.27.0",
    "@xyflow/react": "12.6.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.503.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "10.1.0",
    "remark-gfm": "4.0.1",
    "tailwind-merge": "3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/eslintrc": "3",
    "@eslint/js": "9.24.0",
    "@tailwindcss/postcss": "4.1.4",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/router-devtools": "1.27.0",
    "@tanstack/router-vite-plugin": "1.26.21",
    "@types/node": "20.12.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.0.11",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "4.1.4",
    "tw-animate-css": "1.2.8",
    "typescript": "5.2.2",
    "vite": "5.2.0"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b"
}
