{
  "name": "vite-react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "commitlint": "commitlint --edit",
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore --check \"**/*.+(js|json|ts|tsx)\"",
    "format:fix": "prettier --ignore-path .prettierignore --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "lint": "eslint --max-warnings=0",
    "lint:fix": "eslint --max-warnings=0 --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "validate": "npm-run-all --parallel typecheck lint format",
    "validate-and-build": "npm-run-all validate build",
    "deploy": "npm-run-all validate-and-build serve"
  },
  "dependencies": {
    "@tanstack/react-router": "1.27.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/eslintrc": "3",
    "@eslint/js": "9.24.0",
    "@tanstack/router-devtools": "1.27.0",
    "@tanstack/router-vite-plugin": "1.26.21",
    "@types/node": "20.12.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "autoprefixer": "10.4.19",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tailwindcss": "3.18.0",
    "husky": "9.0.11",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.2.2",
    "vite": "5.2.0"
  }
}
